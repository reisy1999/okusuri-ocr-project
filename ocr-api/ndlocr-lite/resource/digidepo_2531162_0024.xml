<OCRDATASET>
<PAGE IMAGENAME="digidepo_2531162_0024.jpg" WIDTH="2048" HEIGHT="1446">
  <TEXTBLOCK CONF="0.974">
    <LINE TYPE="タイトル本文" X="380" Y="229" WIDTH="189" HEIGHT="22" CONF="0.895" PRED_CHAR_CNT="3.000" ORDER="0" STRING="(ヱ)気送子送付管" />
  <LINE TYPE="本文" X="432" Y="262" WIDTH="558" HEIGHT="31" CONF="0.937" PRED_CHAR_CNT="2.000" ORDER="1" STRING="気送子送付には、上記気送響にて送付するものと、空" />
    <LINE TYPE="本文" X="411" Y="307" WIDTH="579" HEIGHT="30" CONF="0.945" PRED_CHAR_CNT="2.000" ORDER="2" STRING="気の圧縮を使用せず,直接落下させる装置の二通りがあ" />
    <LINE TYPE="本文" X="412" Y="352" WIDTH="558" HEIGHT="28" CONF="0.946" PRED_CHAR_CNT="2.000" ORDER="3" STRING="る。後者の送付管は山納台左側に設置されており.5" />
    <LINE TYPE="本文" X="416" Y="389" WIDTH="579" HEIGHT="33" CONF="0.948" PRED_CHAR_CNT="2.000" ORDER="4" STRING="3./の各層ステーションに直接落下するよう3本の管" />
    <LINE TYPE="本文" X="412" Y="437" WIDTH="581" HEIGHT="29" CONF="0.951" PRED_CHAR_CNT="2.000" ORDER="5" STRING="が通じ投入口のフタに層表示が記されている。取扱いに" />
    <LINE TYPE="本文" X="416" Y="477" WIDTH="580" HEIGHT="31" CONF="0.950" PRED_CHAR_CNT="2.000" ORDER="6" STRING="当っては景送子投入後すみやかにフタを閉め速度を調整" />
    <LINE TYPE="本文" X="416" Y="525" WIDTH="314" HEIGHT="25" CONF="0.930" PRED_CHAR_CNT="3.000" ORDER="7" STRING="する必要がある。(図2参照)" />
    <SHAPE><POLYGON POINTS="410,265,410,551,998,551,998,265" /></SHAPE>
    </TEXTBLOCK>
  <TEXTBLOCK CONF="0.931">
    <LINE TYPE="キャプション" X="522" Y="610" WIDTH="219" HEIGHT="23" CONF="0.921" PRED_CHAR_CNT="3.000" ORDER="8" STRING="(図2)送村官" />
  <SHAPE><POLYGON POINTS="522,610,522,633,741,633,741,610" /></SHAPE>
    </TEXTBLOCK>
  <TEXTBLOCK CONF="0.370">
    <LINE TYPE="本文" X="1152" Y="223" WIDTH="623" HEIGHT="26" CONF="0.937" PRED_CHAR_CNT="2.000" ORDER="9" STRING="b各ステーション(以下5.3./c現在係員の将滅する層" />
    <LINE TYPE="本文" X="1171" Y="270" WIDTH="271" HEIGHT="17" CONF="0.800" PRED_CHAR_CNT="3.000" ORDER="10" STRING="のみステーションと呼ぶ)" />
    <LINE TYPE="タイトル本文" X="1165" Y="309" WIDTH="115" HEIGHT="18" CONF="0.921" PRED_CHAR_CNT="3.000" ORDER="13" STRING="(1)気送管" />
    <SHAPE><POLYGON POINTS="1151,223,1151,249,1775,249,1775,223" /></SHAPE>
    </TEXTBLOCK>
  <TEXTBLOCK CONF="0.961">
    <LINE TYPE="本文" X="1213" Y="354" WIDTH="558" HEIGHT="25" CONF="0.944" PRED_CHAR_CNT="2.000" ORDER="14" STRING="気送子走行中(赤ランプ点灯)でないとき、目的膏の" />
    <LINE TYPE="本文" X="1191" Y="395" WIDTH="566" HEIGHT="26" CONF="0.941" PRED_CHAR_CNT="2.000" ORDER="15" STRING="ボタンを押すと背,赤のランプが同時に点灯するから" />
    <LINE TYPE="本文" X="1190" Y="440" WIDTH="581" HEIGHT="24" CONF="0.945" PRED_CHAR_CNT="2.000" ORDER="16" STRING="投入口の扉を開いて気送子を投入する。気送子が到着す" />
    <LINE TYPE="本文" X="1194" Y="483" WIDTH="305" HEIGHT="21" CONF="0.936" PRED_CHAR_CNT="3.000" ORDER="17" STRING="ると両ランプが同時に消える." />
  <SHAPE><POLYGON POINTS="1189,354,1189,503,1771,503,1771,354" /></SHAPE>
    </TEXTBLOCK>
  <TEXTBLOCK CONF="0.967">
    <LINE TYPE="タイトル本文" X="1159" Y="524" WIDTH="190" HEIGHT="18" CONF="0.905" PRED_CHAR_CNT="3.000" ORDER="18" STRING="(2) 景送子送子管" />
  <LINE TYPE="本文" X="1210" Y="566" WIDTH="560" HEIGHT="25" CONF="0.943" PRED_CHAR_CNT="2.000" ORDER="19" STRING="中央出納台よりの落下による一方交通であるので,各" />
    <LINE TYPE="本文" X="1189" Y="612" WIDTH="579" HEIGHT="23" CONF="0.943" PRED_CHAR_CNT="2.000" ORDER="20" STRING="ステーションは受けー方である。従って気送子の返却は" />
    <LINE TYPE="本文" X="1185" Y="653" WIDTH="144" HEIGHT="19" CONF="0.926" PRED_CHAR_CNT="3.000" ORDER="21" STRING="気送信による。" />
    <SHAPE><POLYGON POINTS="1184,567,1184,672,1771,672,1771,567" /></SHAPE>
    </TEXTBLOCK>
  <TEXTBLOCK CONF="0.980">
    <LINE TYPE="タイトル本文" X="1144" Y="696" WIDTH="106" HEIGHT="17" CONF="0.915" PRED_CHAR_CNT="3.000" ORDER="22" STRING="C気送子" />
  <LINE TYPE="本文" X="1185" Y="741" WIDTH="576" HEIGHT="23" CONF="0.942" PRED_CHAR_CNT="2.000" ORDER="23" STRING="プラスチックの透明な円筒形で出来ており, 頭部はフェ" />
    <LINE TYPE="本文" X="1162" Y="782" WIDTH="603" HEIGHT="25" CONF="0.941" PRED_CHAR_CNT="2.000" ORDER="24" STRING="ルトで保護され,図書請求票を中に入れ(たて二つ折りに" />
    <LINE TYPE="本文" X="1159" Y="825" WIDTH="606" HEIGHT="25" CONF="0.943" PRED_CHAR_CNT="2.000" ORDER="25" STRING="入れる).皮ブタのホックを閉めてから矢印の方向に腰部" />
    <LINE TYPE="本文" X="1162" Y="870" WIDTH="261" HEIGHT="19" CONF="0.934" PRED_CHAR_CNT="3.000" ORDER="26" STRING="を必す下にして投入する。" />
    <SHAPE><POLYGON POINTS="1159,742,1159,889,1767,889,1767,742" /></SHAPE>
    </TEXTBLOCK>
