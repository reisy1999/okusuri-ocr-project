    <LINE TYPE="本文" X="179" Y="825" WIDTH="205" HEIGHT="12" CONF="0.837" PRED_CHAR_CNT="3.000" ORDER="48" STRING="画像に対して機械学習を適用した実験" />
  <LINE TYPE="本文" X="191" Y="842" WIDTH="181" HEIGHT="12" CONF="0.854" PRED_CHAR_CNT="3.000" ORDER="49" STRING="背景白色化による紙の変色の除去" />
